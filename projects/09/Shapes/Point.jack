class Point {
    field int x,y;

    constructor Point new(int Px, int Py) {
        let x = Px;
        let y = Py;
        return this;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
    }

    method int get_x(){
        return x;
    }

    method int get_y(){
        return y;
    }

}
